FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "p|pcorr|pcorrFinal|pFinal"
    {
        solver          GAMG;
        tolerance       1e-08;
        relTol          0;
        minIter         1;
        maxIter         1000;
        smoother        GaussSeidel;
        nPreSweeps      0;
        nPostSweeps     2;
        nFinestSweeps   2;
        scaleCorrection true;
        directSolveCoarsest false;
        cacheAgglomeration true;
        nCellsInCoarsestLevel 20;
        agglomerator    faceAreaPair;
        mergeLevels     1;
    }

    "U|UFinal"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-08;
        relTol          0;
        minIter         1;
    }
}

PIMPLE
{
    //correctPhi          no;
    nOuterCorrectors    100;
    nCorrectors         3;
    nNonOrthogonalCorrectors 2;

    //pRefCell        0;
    pRefPoint (0.5 0.5 0);
    pRefValue 0.0;

    moveMeshOuterCorrectors true;
    consistent true;

    residualControl
    {
        p
        {
            tolerance 1e-6;
            relTol 0;
        }
        U
        {
            tolerance 1e-7;
            relTol 0;
        }
    }
}

relaxationFactors
{}

