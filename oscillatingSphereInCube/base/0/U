FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    sphere
    {
        // `newMovingWallVelocity` implements boundary non-orthogonal
        // corrections, which are required for second-order accuracy.
        type            newMovingWallVelocity;
        value           uniform (0 0 0);
    }

    "xmin|xmax|ymin|ymax|zmin|zmax"
    {
        // `fixedValueCorrected` employs non-orthogonal corrections which are
        // required to achieve second-order accuracy on grids with
        // boundary non-orthogonality.
        type            fixedValueCorrected;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
